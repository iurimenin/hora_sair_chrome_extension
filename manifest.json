{
  "name": "Hora sair - Ponto Mais Web",
  "version": "1.0",
  "description": "Extens√£o para calcular a hora que pode sair baseado nos pontos do ponto mais web.",
  "permissions": [
    "declarativeContent",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/calendar_check_18dp.png",
      "32": "images/calendar_check_36dp.png",
      "48": "images/calendar_check_48dp.png",
      "128": "images/calendar_check_48dp.png"
    }
  },
  "icons": {
    "16": "images/calendar_check_18dp.png",
    "32": "images/calendar_check_36dp.png",
    "48": "images/calendar_check_48dp.png",
    "128": "images/calendar_check_48dp.png"
  },
  "manifest_version": 2
}
